    <thead class="">
        <tr class="text-center table-dark ">
            <th>id</th>
            <th>username</th>
            <th>fullname</th>
        </tr>
        <% data.forEach((item)=>{ %>
        <tr>
            <td><%= item.use_id %></td>
            <td><%= item.use_username %></td>
